<% content_for :page_title do %>
  <%= Spree.t(:new_seo_page) %>
<% end %>

<% content_for :page_actions do %>
  <li>
    <%= button_link_to Spree.t(:back_to_seo_pages_list), spree.admin_seo_pages_path, :icon => 'arrow-left' %>
  </li>
<% end %>

<div data-hook="admin_gift_page_new_form_header">
  <%= render :partial => 'spree/shared/error_messages', :locals => { :target => @seo_page } %>
</div>

<div data-hook="admin_gift_page_new_form">
  <%= form_for [:admin, @seo_page], url: admin_seo_pages_url, method: :post do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>

   <%= button "update", 'icon-refresh' %>
  <% end %>
</div>